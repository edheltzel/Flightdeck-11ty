{
  "name": "flightdeck-11ty",
  "version": "0.0.4",
  "description": "A starter project for Eleventy using an opinionated and minimal frontend workflow.",
  "scripts": {
    "start": "run-p dev:11ty dev:parcel",
    "dev:11ty": "eleventy --serve --config=.flightdeck.js",
    "dev:parcel": "run-p  watch:assets",
    "watch:assets": "parcel watch './src/assets/images/*' ./src/assets/js/app.js ./src/assets/styles/app.scss --dist-dir ./dist/assets",
    "build": "run-s clean build:parcel build:11ty",
    "build:11ty": "cross-env ELEVENTY_ENV=production eleventy",
    "build:parcel": "parcel build './src/assets/images/*' ./src/assets/js/app.js ./src/assets/styles/app.scss --dist-dir ./dist/assets",
    "clean": "./.scrub site",
    "purge": "./.scrub purge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edheltzel/Flightdeck-11ty.git"
  },
  "keywords": [],
  "author": "Ed Heltzel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edheltzel/Flightdeck-11ty/issues"
  },
  "homepage": "https://github.com/edheltzel/Flightdeck-11ty#readme",
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@parcel/config-default": "^2.7.0",
    "@parcel/core": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-embed-everything": "^1.14.1",
    "html-minifier": "^4.0.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-webp": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "sass": "^1.55.0",
    "sharp": "^0.29.3"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0"
  }
}
