{
  "name": "flightdeck-11ty",
  "version": "0.0.3",
  "description": "A starter project for Eleventy using an opinionated and minimal frontend workflow.",
  "scripts": {
    "start": "npm-run-all -p dev:11ty dev:images",
    "dev:11ty": "eleventy --serve",
    "dev:images": "watch 'pnpm run build:images' ./src/assets/images",
    "build:images": "imagemin src/assets/images/* --out-dir=dist/assets/images --plugin.webp.quality=0.5",
    "clean:node": "rm -rf node_modules package-lock.json pnpm-lock.yaml",
    "clean": "rm -rf dist/ .pnpm-debug.log",
    "purge": "pnpm run clean:node && pnpm run clean && echo \"\\033[32mðŸ§¹ All Clean âœ¨\"\"\\033[0m Run \"\"\\033[33mpnpm install\"\"\\033[0m to start fresh ðŸ¤©\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edheltzel/Flightdeck-11ty.git"
  },
  "keywords": [],
  "author": "Ed Heltzel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edheltzel/Flightdeck-11ty/issues"
  },
  "homepage": "https://github.com/edheltzel/Flightdeck-11ty#readme",
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "autoprefixer": "^10.4.7",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-embed-everything": "^1.14.0",
    "esbuild": "^0.14.47",
    "esbuild-sass-plugin": "^2.2.6",
    "html-minifier": "^4.0.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-webp": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.1",
    "sass": "^1.52.3",
    "watch": "^1.0.2"
  }
}
